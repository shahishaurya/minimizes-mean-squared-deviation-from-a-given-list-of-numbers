def test(nums):
    return sum(nums) / len(nums) 
nums = [4, -5, 17, -9, 14, 108, -9]
print("Original list:")
print(nums)
print("Minimizes mean squared deviation from the said list of numbers:")
print(test(nums))
nums = [12, -2, 14, 3, -15, 10, -45, 3, 30]
print("Original list:")
print(nums)
print("Minimizes mean squared deviation from the said list of numbers:")
print(test(nums))
